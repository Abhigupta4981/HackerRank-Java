package enumExample;

public class Main {

	public static void main(String[] args) {
		System.out.println("Enum Example");
		
		/* Create a new person with Black hair */
		Person peterParker = new Person(HairColors.BLACK);
		System.out.println("Creating Peter Parker with Black hair");
		System.out.println(peterParker.hairColor);

	}

}
